{
  "name": "web-search-zaimcp",
  "version": "0.1.1",
  "description": "Minimal CommonJS helper to call BigModel MCP webSearchPrime with connection reuse and robust parsing.",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.mjs",
      "require": "./src/index.js",
      "default": "./src/index.js"
    }
  },
  "files": [
    "src",
    "README.md"
  ],
  "scripts": {
    "test": "pnpm -s typecheck",
    "typecheck": "tsc -p tests/types/tsconfig.json",
    "example": "node examples/run-web-search.js",
    "example:esm": "node examples/run-web-search.mjs"
  },
  "keywords": [
    "mcp",
    "bigmodel",
    "web-search",
    "agent-tools"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.21.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "dotenv": "^17.2.3",
    "typescript": "~5.6.3"
  }
}
